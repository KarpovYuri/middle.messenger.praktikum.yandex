{
  "extends": "stylelint-config-standard",
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "color-hex-case": "lower",
    "import-notation": "string",
    "no-empty-source": null,
    "at-rule-no-unknown": null,
    "declaration-empty-line-before": "never"
  },
  "ignoreFiles": [
    ".parcel-cache/*",
    "dist/*"
  ]
}
