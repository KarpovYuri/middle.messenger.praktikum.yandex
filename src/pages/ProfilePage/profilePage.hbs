<form class="form">
  <label class="avatar">
    <input class="avatar__input" type="file" id="avatar" name="avatar" accept="image/*">
    <img class="avatar__img" src={{#if avatar}}{{avatar}}{{else}}{{noAvatar}}{{/if}} alt="Аватар">
  </label>
  {{{title}}}
  <fieldset class="form__fieldset form__fieldset_columns_two">
    {{{first_name}}}
    {{{second_name}}}
    {{{display_name}}}
    {{{login}}}
    {{{email}}}
    {{{phone}}}
    {{{old_password}}}
    {{{new_password}}}
  </fieldset>
  {{{button}}}
  {{{linkBack}}}
  {{{logoutLink}}}
</form>
